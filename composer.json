{
    "name": "bytenetizen/fast-replace",
    "description": "Fast Replace text for the Laravel framework.",
    "keywords": ["bytenetizen", "laravel", "Placeholder", "piece", "Transform Text", "Fast Replace", "Replace"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Valerii Petrov",
            "email": "prodcoders@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^9.21|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bytenetizen\\FastReplace\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bytenetizen\\FastReplace\\Providers\\ReplaceServiceProvider"
            ],
            "aliases": {
                "Replace": "Bytenetizen\\FastReplace\\Facades\\Replace"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
